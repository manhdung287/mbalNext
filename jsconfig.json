{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES6",
    "paths": {
      "lib/*": ["./src/lib/*"],
      "res/*": ["./src/res/*"],
      "core/*": ["./src/core/*"],
      "routers/*": ["./src/routers/*"],
      "screens/*": ["./src/screens/*"],
      "config/*": ["./src/config/*"],
      "services/*": ["./src/services/*"],
      "utils/*": ["./src/utils/*"],
      "modules/*": ["./src/modules/*"],
      "pages/*": ["./src/pages/*"],
      "styles/*": ["./src/styles/*"]
    }
  },
  "exclude": ["node_modules", "build", "dist", ".next"]
}
